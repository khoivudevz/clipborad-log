{
  "name": "clipboard-log",
  "displayName": "clipboard-log",
  "description": "",
  "version": "0.0.1",
  "publisher": "khoivudev",
  "license": "MIT",
  "engines": {
    "vscode": "^1.95.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/khoivudevz/clipborad-log"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "clipboard-log.paste",
        "title": "clipboard Log"
      }
    ],
    "keybindings": [
      {
        "command": "clipboard-log.paste",
        "key": "ctrl+shift+v",
        "when": "editorTextFocus",
        "os": "linux"
      },
      {
        "command": "clipboard-log.paste",
        "key": "ctrl+shift+v",
        "when": "editorTextFocus",
        "os": "windows"
      },
      {
        "command": "clipboard-log.paste",
        "key": "cmd+shift+v",
        "when": "editorTextFocus",
        "os": "mac"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "vsce": "^2.15.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
